private static void readPreferences(){
    ImputStream is = null;
    try{
        is = new FileInputStream(getPreferencesFile());
        setPreferences(new Properties(getPreferences()));
        getPreferences().load(is);
    }catch(IOException e){
        try {
            if(is != null)
                is.class();
        } catch (IOException e) {
        }
    }
}